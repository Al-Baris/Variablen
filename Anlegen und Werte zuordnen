using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Variablen
{
    class Program
    {
        static void Main(string[] args)
        {
            byte zahl_byte = 35;
            sbyte zahl_sbyte = -60;
            int zahl_int = 2147483;
            uint zahl_uint = 4294967;
            short zahl_short = -30600;
            ushort zahl_ushort = 65535;
            long zahl_long = -9223372036854775;
            ulong zahl_ulong = 8;
            float zahl_float = -340003f;
            double zahl_double = 494949949;
            char zahl_char = '#';
            string meinText = "Ich kontrolliere Variablen";
            string zahlenWert = "35";

            int meinZahlAusText = int.Parse(zahlenWert);
            Console.WriteLine(meinZahlAusText);
            Console.ReadLine();
        }
    }
}
